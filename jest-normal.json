{
    "roots": [
        "<rootDir>/src"
    ],
    "moduleFileExtensions": [
        "js",
        "jsx",
        "json",
        "ts",
        "tsx"
    ],
    "testMatch": [
        "**/*.test.*"
    ],
    "transform": {
        ".*\\.[tj]sx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
}